<?xml version="1.0" encoding="utf-8" ?>
<Rules FriendlyName="Codemanship's Code Craft Rules">
  <Rule TypeName="IdentifierLengthRule" Category="CodeCraft.FxCop" CheckId="TT1011">
    <Name>Identifier Length</Name>
    <Description>
      Long names are harder to read and harder to use 
    </Description>
    <Url></Url>
    <Resolution Name="Rename">Consider renaming this type, method, variable or constant</Resolution>
    <MessageLevel Certainty="95">Warning</MessageLevel>
    <Email></Email>
    <FixCategories>Breaking</FixCategories>
    <Owner></Owner>
  </Rule>
</Rules>
